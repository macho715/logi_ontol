hvdc:hasAdnocCode a rdf:Property ;
    rdfs:label "has ADNOC code"@en ;
    rdfs:range xsd:string .

hvdc:hasYardArea a rdf:Property ;
    rdfs:label "has yard area"@en ;
    rdfs:range xsd:decimal .

hvdc:managedBy a rdf:Property ;
    rdfs:label "managed by"@en ;
    rdfs:range xsd:string .

